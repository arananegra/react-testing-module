{
  "name": "react-testing-module",
  "version": "1.0.0",
  "description": "Excercise for react-test-by-example",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack/dev.js",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config ./config/webpack/prod.js",
    "test": "jest -c ./config/test/jest.json --verbose",
    "test:watch": "jest -c ./config/test/jest.json --verbose --watchAll -i",
    "pretest:coverage": "rimraf coverage",
    "test:coverage": "jest -c ./config/test/jest.coverage.json --verbose --coverage"
  },
  "author": "Lemoncode",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "@material-ui/icons": "^4.2.1",
