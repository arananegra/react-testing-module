    "jest": "^24.9.0",
    "react-test-renderer": "^16.9.0",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.5.0",
    "webpack-merge": "^4.2.1"
  }
