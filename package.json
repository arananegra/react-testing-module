    "connected-react-router": "^6.5.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux-saga": "^1.0.5",
    "regenerator-runtime": "^0.13.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@testing-library/react": "^9.1.3",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
